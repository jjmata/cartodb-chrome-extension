{
  "manifest_version": 2,
  "name": "CartoDB",
  "description": "One-click map creation from any table!",
  "version": "0.4",
  "browser_action": {
    "default_icon": "cartodb.png",
    "default_popup": "cartodb-chrome-popup.html"
  },
  "icons": {
    "16": "cartodb.png",
    "48": "cartodb.png",
    "128": "cartodb.png"
  },
  "permissions": [
    "https://www.cartodb.com/",
    "http://development.localhost.lan/",
    "contextMenus",
    "storage",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [ "helpers.js", "cartodb-chrome-background.js", "cartodb.js" ]
  },
  "content_scripts": [
  {
    "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
    "css": ["cartodb-chrome.css"],
    "js": ["helpers.js", "cartodb-chrome.js", "sprintf.min.js", "cartodb.js"  ],
    "all_frames": true
  } ],
  "web_accessible_resources": [
    "sprintf.min.js",
    "sprintf.min.map",
    "*.png"
  ]
}
